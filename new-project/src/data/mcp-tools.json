    "开发工具": {
      "name": "开发工具",
      "description": "提升开发工作流程和环境管理的工具",
      "tools": [
        {
          "id": "figma-context-mcp",
          "title": "Figma Context MCP",
          "description": "为代码助手提供直接访问Figma数据的能力，帮助实现一次性设计实现",
          "imageUrl": "/images/figma-mcp.jpg",
          "link": "https://github.com/GLips/Figma-Context-MCP",
          "tags": [
            "设计",
            "开发",
            "Figma"
          ],
          "installation": {
            "auto": {
              "command": "npx figma-developer-mcp --figma-api-key=<your-figma-api-key>"
            },
            "manual": {
              "command": "npx",
              "args": [
                "figma-developer-mcp",
                "--figma-api-key=YOUR_API_KEY"
              ],
              "env": {
                "FIGMA_API_KEY": "YOUR_API_KEY"
              }
            }
          }
        },
        {
          "id": "docker-mcp",
          "title": "Docker MCP",
          "description": "通过MCP进行Docker容器管理和操作",
          "imageUrl": "/images/docker-mcp.jpg",
          "link": "https://github.com/QuantGeekDev/docker-mcp",
          "tags": [
            "Docker",
            "容器化",
            "DevOps"
          ],
          "installation": {
            "auto": {
              "command": "npx docker-mcp-server init"
            },
            "manual": {
              "command": "npx",
              "args": [
                "docker-mcp-server",
                "init"
              ],
              "env": {
                "_placeholder": ""
              }
            }
          }
        },
